{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "starterkit-adf-wus"
		},
		"GitHub1_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub1'"
		},
		"starterkitdatalakestoragegen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'starterkitdatalakestoragegen2'"
		},
		"starterkitgithub_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'starterkitgithub'"
		},
		"GitHub1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "StarterKit2K25"
		},
		"starterkitdatalakestoragegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://starterkitstgwus.dfs.core.windows.net/"
		},
		"starterkitgithub_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "StarterKit2K25"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/GitHub1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "GitHub",
				"typeProperties": {
					"username": "[parameters('GitHub1_properties_typeProperties_username')]",
					"credential": {
						"type": "SecureString",
						"value": "[parameters('GitHub1_credential')]"
					},
					"encryptedCredential": "ew0KICAiVmVyc2lvbiI6ICIyMDE3LTExLTMwIiwNCiAgIlByb3RlY3Rpb25Nb2RlIjogIktleSIsDQogICJTZWNyZXRDb250ZW50VHlwZSI6ICJQbGFpbnRleHQiLA0KICAiQ3JlZGVudGlhbElkIjogIkRBVEFGQUNUT1JZQDYwNUE4OTg1LTk5N0ItNDgzNS04NTgyLTY5NzZGQUVGN0MyQl83YTc4MWJiYS04MzQ5LTRhNTEtODFjNC0yOWQ3M2ZiMTAyY2UiDQp9"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/starterkitdatalakestoragegen2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('starterkitdatalakestoragegen2_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('starterkitdatalakestoragegen2_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/starterkitgithub')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "GitHub",
				"typeProperties": {
					"username": "[parameters('starterkitgithub_properties_typeProperties_username')]",
					"credential": {
						"type": "SecureString",
						"value": "[parameters('starterkitgithub_credential')]"
					},
					"encryptedCredential": "ew0KICAiVmVyc2lvbiI6ICIyMDE3LTExLTMwIiwNCiAgIlByb3RlY3Rpb25Nb2RlIjogIktleSIsDQogICJTZWNyZXRDb250ZW50VHlwZSI6ICJQbGFpbnRleHQiLA0KICAiQ3JlZGVudGlhbElkIjogIkRBVEFGQUNUT1JZQDYwNUE4OTg1LTk5N0ItNDgzNS04NTgyLTY5NzZGQUVGN0MyQl9kZTBhMGRhOC1mZWY0LTQzN2MtYTk0Ny01YTUzZmRhMDZkNmUiDQp9"
